input::-webkit-input-placeholder{
	color:#278687;
}
input::-moz-placeholder{
	color:#278687;
}
input:-ms-input-placeholder{
	color:#278687;
}
input:focus::-webkit-input-placeholder{
	color:transparent;
}
input:focus::-moz-placeholder{
	color:transparent;
}
input:focus:-ms-input-placeholder{
	color:transparent;
}
textarea::-webkit-input-placeholder{
	color:#278687;
}
textarea::-moz-placeholder{
	color:#278687;
}
textarea:-ms-input-placeholder{
	color:#278687;
}
textarea:focus::-webkit-input-placeholder{
	color:transparent;
}
textarea:focus::-moz-placeholder{
	color:transparent;
}
textarea:focus:-ms-input-placeholder{
	color:transparent;
}
.form-control::-moz-placeholder {
  color: #278687;
  opacity: 1;
}
.form-control:-ms-input-placeholder {
  color: #278687;
}
.form-control::-webkit-input-placeholder {
  color: #278687;
}
.ui-select-bootstrap .ui-select-toggle{
	color:#2e93fb; 
	background: #0c2936;
}
.window .form-validate .ui-select-bootstrap .ui-select-toggle,
.window .form-validate .form-control{
	border-radius: 0;
	background-color: #081421;
	border: solid 1px rgba(58,105,247,.35);
}
.form-control:focus {
  border-color: #1284f5;
  box-shadow: inset 0 1px 1px rgba(18,132,245,.075), 0 0 8px rgba(18,132,245, 0.6);
}
/*弹框input 状态*/
.ui-select-match.btn-default-focus{
	/*border:1px solid #0f82bd;*/
	border: none;
}
.btn-default,.ui-select-bootstrap .ui-select-toggle{
	border-color: #1284F5;
}
.window .form-validate .form-control:focus,
.btn-default-focus{
	border-radius: 0;
	border:1px solid #0f82bd;
	box-shadow: 0 0 0.2rem #0f82bd;
	font-size: .14rem;
}
.window .form-validate .form-control:disabled{
	background-color: #0a1b2b;
	border: solid 1px rgba(77,220,225,.15);
	color: #d5e92;
}
/*弹框input 状态*/
.form-control.inp_default2[readonly]{
	color:#1284f5;
	opacity: 1;
	background: #032343;
}
.form-validate .form-control[disabled],
.form-validate .form-validate fieldset[disabled]{
	opacity: .9;
	background: none;
}

.ngGrid .table > thead > tr > th{
	border-bottom: 1px solid #0f7c7f;
}
.ngGrid .table > tbody > tr:hover,
.ngGrid .table > tbody > tr:focus,
.ngGrid .table > tbody > tr.active{
	height: 0.44rem;
	border-right: none;
	border-left: none;
	cursor: pointer;
	box-sizing: border-box;
	background: #077879;
}
 .table > tbody > tr > td,
 .table thead tr th,
 .tablef > tbody > tr > td{
 	font-size: 0.18rem;
}
.c-checkbox span,
.c-radio span{
	border: 1px solid rgba(27,152,207,0.8);
}
.c-checkbox:hover span,
.c-radio:hover span {
  border-color: #5d9cec;
}
/*form 表单*/
.tooltip-inner .form-control[disabled]{
	background: #032343;
	border-color:#18abf9; 
	box-shadow:0 0 0.01rem #18abf9; 
}
.btn_bg,
.tooltip-inner .input-group-btn .btn-default,
.bgTitle,.nav-tabs,
.inp_default2,.form-control.inp_default2{
	background: #032343;
}
.tooltip-inner .input-group-btn .btn-default{
	border:1px solid #18abf9; 
}
/*tab切换*/
.nav.nav-tabs > li{
	border-right: 0.01rem solid #2e93fb;
}
.nav.nav-tabs > li:last-child{
	border-right: none;
}
.nav-tabs > li.active > a, 
.nav-tabs > li.active > a:hover, 
.nav-tabs > li.active > a:focus{
	color: #2e93fb;
}
.nav-tabs{
	border: 1px solid #1284f5;
	background-color: #032343;
}
.nav.nav-tabs > li.active > a, 
.nav.nav-tabs > li.active > a:hover,
.nav.nav-tabs > li.active > a:focus{
	background-color: #021323;
	color: #2e93fb;
}
.nav.nav-tabs > li > a:hover, 
.nav.nav-tabs > li > a:focus{
	box-shadow: 0 0 7px 1px rgba(74,213,253,.67);
	-moz-box-shadow: 0 0 7px 1px rgba(74,213,253,.67);
	box-shadow: 0 0 7px 1px rgba(74,213,253,.67);
	background-color: #021323;
	color: #2e93fb;
}
.inp_boxs .ui-select-bootstrap > .ui-select-choices.dropdown-menu{
	background: #080f22;
    border: 1px solid #1284f5;
    border-radius: 0;
    z-index: 1000;
}
.inp_boxs .ui-select-bootstrap > .ui-select-choices.dropdown-menu .ui-select-choices-row>a{
	color: #2e93fb;
}
.inp_boxs .ui-select-bootstrap .ui-select-choices-row.active>a{
	background: #174363;
}
