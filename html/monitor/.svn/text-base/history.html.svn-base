							<label class="LabelTextColor fl pt-sm">日期</label>
							<input type="text" class="form-control date_icon fl color_blue" id="startTime" ng-model="startTime1" readonly/>
						</div>
						<div class="Caleline3"></div>
						<div class="DateBox_list2 DateBox_list3 clearfix">
							<label class="LabelTextColor fl pt-sm">日期</label>
							<input type="text" class="form-control date_icon fl color_blue" id="endTime" ng-model="endTime1" readonly/>
						</div>
			</div>
            <button class="fl History_searchBtn borderNone" ng-click="searchHistory()">查询</button>
            
            
            <!--<div class="playsExitBtn fr">-->
