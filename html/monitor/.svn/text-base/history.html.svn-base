                </button>
                <button class="next" ng-click="setCurrentIndex(1)">后退</button>
            </div>
            <div class="player playerMiddle fl">
                <ng-range ng-range-min="0" ng-range-step="1" ng-range-max="{{rangeMax}}"
                          acc="acc" rangs="rangeMax"
                          range-changed="setCurrentIndex()" ng-model="range"></ng-range>
            </div>
