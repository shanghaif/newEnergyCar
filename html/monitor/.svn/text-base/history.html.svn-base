    <div class="smallPlayer" id="smallPlayer">
        <div class="playerTop clearfix">
            <div class="playKeys clearfix fl">
                <button class="prev" ng-click="setCurrentIndex(-1)">前进</button>
                <button class="start" ng-class="{'map_tstart':pause,'map_tstop':!pause}"
