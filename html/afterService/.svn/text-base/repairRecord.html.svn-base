						<div class="RepairTable_list_center fl">
							<table class="table Table_trlist">
								<thead class="thead_bg">
									<tr>
										<th class="Repair_tabfirth backImg_common1">
											<span class="thfir_text">操作</span>
										</th>
										<th class="Repair_theadth2">报修单号</th>
										<th>维修状态</th>
										<th>车牌号</th>
										<th>VIN码</th>
										<th>SIM卡ICCID号</th>
										<th>故障类型</th>
										<th>故障等级</th>
										<th>故障地点</th>
										<th>故障描述</th>
										<th>预约维修站点</th>
										<th>报修日期</th>
										<th>报修人</th>
										<th>报修人电话</th>
										<th>维修开始日期</th>
										<th>维修结束日期</th>
										<th>实际维修站点</th>
										<th>维修人</th>
										<th>维修方案</th>
										<th>备注</th>
									</tr>
								</thead>
								<tbody class="Tr_hoverlist">
									<tr ng-repeat="x in recordList" ng-class="{active:activeIndex==$index}" ng-click="changeIndex($index)" class="{{ $even ? 'BackColor1even': 'BackColor1odd'}}">
										<td>
											<span class="Btn_colorGreen borderNone modifyButton_top RepairModify_btn  td_color" ng-click="repairLook_open(x.workformCode,'modify')">修改</span>
											<button ng-click='managementDelete(x)' class="Btn_colorGreen borderNone  modifyButton_top RepairModify_btn ml-sm">删除</button>
										</td>
										<td>
											<!--href="#/Repair_LookOpen/{{x.workformCode}}/look" target="_blank" -->
											<span class="td_color Text_decoration" ng-click="repairLook_open(x.workformCode,'look')">{{x.workformCode}}</span>
										</td>
										<td >
											<span ng-if="x.status == 0">待处理</span>
											<span ng-if="x.status == 1">处理中</span>
											<span ng-if="x.status == 2">已完成</span>
										</td>
										<td>{{x.licenseNumber}}</td>
										<td>{{x.vin}}</td>
										<td>{{x.iccid}}</td>
										<td>{{x.faulType||""}}</td>
										<td>{{x.faultRank}}</td>
										<td>{{x.faultAddress|textLengthSet:true:20:'...'}}</td>
										<td>{{x.faultDescription|textLengthSet:true:20:'...'}}</td>
										<td>{{x.siteName}}</td>
										<td>{{x.creaTime}}</td>
										<td>{{x.operator|textLengthSet:true:20:'...'}}</td>
										<td>{{x.phone}}</td>
										<td>{{x.starTime | date:'yyyy-MM-dd'}}</td>
										<td>{{x.endTime | date:'yyyy-MM-dd'}}</td>
										<td>{{x.realSiteName}}</td>
										<td>{{x.maintainer}}</td>
										<td>{{x.program|textLengthSet:true:20:'...'}}</td>
										<td>{{x.remark|textLengthSet:true:20:'...'}}</td>
									</tr>
								</tbody>
							</table>
						</div>
